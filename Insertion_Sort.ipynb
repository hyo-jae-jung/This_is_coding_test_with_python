   "source": [
    "arr = [7,5,9,0,3,1,6,2,4,8]\n",
    "\n",
    "for i in range(1,len(arr)):\n",
    "    for j in range(i,0,-1):\n",
    "        if arr[j] < arr[j-1]:\n",
    "            arr[j],arr[j-1] = arr[j-1],arr[j]\n",
    "        else:\n",
    "            break\n",
    "\n",
    "print(arr)"
   ]
  },
  {
