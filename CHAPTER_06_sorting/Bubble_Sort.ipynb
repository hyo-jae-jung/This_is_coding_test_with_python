   "source": [
    "arr = [5,3,4,1,2]\n",
    "n = len(arr)\n",
    "\n",
    "print(arr)\n",
    "for i in range(n):\n",
    "    for j in range(n - i - 1):\n",
    "        if arr[j] > arr[j + 1]:\n",
    "            arr[j], arr[j + 1] = arr[j + 1], arr[j]\n",
    "    print(arr)\n"
   ]
  }
